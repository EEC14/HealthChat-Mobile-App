{
  "expo": {
    "name": "HealthChat",
    "slug": "HealthChat",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#000"
    },
    "scheme": "HealthChat",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shebl.HealthChat"
    },
    "android": {
      "versionCode": 2,
      "splash": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "resizeMode": "cover",
        "backgroundColor": "#000"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "resizeMode": "cover",
        "backgroundColor": "#000"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.shebl.HealthChat"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "cover",
          "backgroundColor": "#000"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff",
          "userInterfaceStyle": "automatic",
          "permissions": ["alert", "badge", "notification"],
          "permissionsAlwaysAsk": {
            "alert": true,
            "badge": true,
            "sound": true,
            "provisional": true
          },
          "sound": true,
          "sounds": ["./assets/sounds/notification1.wav"],
          "androidMode": "default",
          "androidChannelId": "default",
          "androidChannelName": "default"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "72fd329a-362e-4f69-86e7-43ed282749fa"
      }
    }
  }
}
