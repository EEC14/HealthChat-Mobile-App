{
  "expo": {
    "name": "HealthChat",
    "slug": "HealthChat",
    "version": "1.0.21",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "scheme": "healthchat",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.healthchatdev.HealthChat",
      "infoPlist": {
        "NSHealthShareUsageDescription": "This app requires access to health data for tracking fitness activities and recovery metrics.",
        "NSHealthUpdateUsageDescription": "This app requires permission to write workout and health metrics data."
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "versionCode": 2,
      "splash": {
        "foregroundImage": "./assets/images/splash-icon.png"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000"
      },
      "package": "com.healthchatdev.HealthChat",
      "permissions": [
        "android.permission.ACTIVITY_RECOGNITION",
        "android.permission.ACCESS_FINE_LOCATION",
        "com.google.android.gms.permission.ACTIVITY_RECOGNITION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#000"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon-app.png",
          "userInterfaceStyle": "automatic",
          "permissions": [
            "alert",
            "badge",
            "notification"
          ],
          "permissionsAlwaysAsk": {
            "alert": true,
            "badge": true,
            "sound": true,
            "provisional": true
          },
          "sound": true,
          "sounds": [
            "./assets/sounds/notification1.wav"
          ],
          "androidMode": "default",
          "androidChannelId": "default",
          "androidChannelName": "Default Channel"
        }
      ],
      "expo-font",
      [
        "react-native-health",
        {
          "isClinicalDataEnabled": true,
          "healthSharePermission": "This app requires access to health data for tracking fitness activities and recovery metrics.",
          "healthUpdatePermission": "This app requires permission to write workout and health metrics data."
        }
      ],
      [
        "react-native-google-fit",
        {
          "googleFitPermission": "This app requires permission to access your fitness data to provide personalized recommendations."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f7838caa-f9c6-4831-b8b9-939bb08bdd7a"
      }
    },
    "owner": "healthchat-dev",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f7838caa-f9c6-4831-b8b9-939bb08bdd7a"
    }
  }
}
