{
  "expo": {
    "name": "HealthChat",
    "slug": "HealthChat",
    "version": "1.0.18",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "scheme": "healthchat",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.healthchatdev.HealthChat"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "versionCode": 2,
      "splash": {
        "foregroundImage": "./assets/images/splash-icon.png"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000"
      },
      "package": "com.healthchatdev.HealthChat"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#000"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon-app.png",
          "userInterfaceStyle": "automatic",
          "permissions": ["alert", "badge", "notification"],
          "permissionsAlwaysAsk": {
            "alert": true,
            "badge": true,
            "sound": true,
            "provisional": true
          },
          "sound": true,
          "sounds": ["./assets/sounds/notification1.wav"],
          "androidMode": "default",
          "androidChannelId": "default",
          "androidChannelName": "Default Channel"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f7838caa-f9c6-4831-b8b9-939bb08bdd7a"
      }
    },
    "owner": "healthchat-dev",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f7838caa-f9c6-4831-b8b9-939bb08bdd7a"
    }
  }
}
